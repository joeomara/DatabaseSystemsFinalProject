<Window x:Class="GameBracketManager.PlayerForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameBracketManager"
        mc:Ignorable="d"
        Title="PlayerForm" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="97*"/>
            <ColumnDefinition Width="195*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Name" Grid.ColumnSpan="2" Margin="0, 10" />
        <TextBox Height="25" Grid.Column="1" Margin="0,10" VerticalAlignment="Top" />
        <Label Margin="0,40" Content="DOB" Grid.ColumnSpan="2" />
        <TextBox Height="25" Grid.Column="1" Margin="0,40" VerticalAlignment="Top" />
    </Grid>
</Window>
